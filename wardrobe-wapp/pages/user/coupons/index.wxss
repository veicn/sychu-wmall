page{
    height: 100%;
}
.title-box{
    width: 100%;
    padding-top: 330rpx;
    text-align: center;
    font-size:28rpx;
    color:#999;
    background: url("data:image/png;base64,") no-repeat center 205rpx;
    background-size: 100rpx auto;
    margin-bottom: 50rpx;
}
.coupons-list {
    padding: 20rpx 40rpx;
}
.coupon-item .nick {
    padding:.66% 0;
    color: #fff;
}
.coupon-item .coupon-money {
    width: 100%;
    display: flex;
    font-size: 30rpx;
    align-items: center;
}
.coupon-item .coupon-money .tit {
    font-size: 60rpx;
}
.coupon-item .coupon-money .lay {
    flex: 1;
}
.style-one {
    width:100%;
    height:200rpx;
    position:relative;
    margin:5% 0;
    display:-webkit-box;
    display:-webkit-flex;
    display:flex;
    align-items:center;
    justify-content:center;
    background-color:#fff;
    border:1px solid #E5004F;
}
.style-one .info-box {
    flex:1;
    padding:0 3% 0 10%;
    position:relative;
}
.style-one .get-btn {
    display: flex;
    align-items: center;
    justify-content: center;
    width:24%;
    height:200rpx;
    text-align:center;
    color:#fff;
    font-size:30rpx;
    line-height:1.35;
    background-color:#E5004F;
    position:relative;
}
.style-one .nick,.style-four .nick {
    color:#c1c1c1;
}
.style-one .of,.style-one .lay .tit {
    color:#E5004F;
}
.style-one .lay .demand {
    color:#454545;
    margin-left: 16rpx;
}