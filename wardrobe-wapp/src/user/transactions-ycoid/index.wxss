page{
    height: 100%;
}
.container{
    background-color: #f5f5f9;
    justify-content: initial;
}
.no-order{
    width: 100%;
    position: absolute;
    bottom: 0;
    top: 88rpx;
    left: 0;
    right: 0;
    text-align: center;
    padding-top: 203rpx;
    background-color: #F2f2f2;
}
.no-order-img{
    width: 81rpx;
    height: 96rpx;
    margin-bottom: 31rpx;
}
.no-order .text{
    font-size:28rpx;
    color:#999999;
    text-align: center
}

.transactions-list {
    width: 100%;
}
.transactions-list .item {
    display: flex;
    padding: 20rpx 30rpx;
    background-color: #FFFFFF;
    border-top: 1rpx solid #DCDCDC;
}
.transactions-list .item .item-left {
    flex: 1;
    width: 50%;
}
.transactions-list .item .transactions-balance {
    color: #e64340;
}
.transactions-list .item .transactions-balance.plus {
    color: #179B16;
}
.transactions-list .item .transactions-type {
    color: #999999;
    font-size: 24rpx;
}
.transactions-list .item .transactions-time {
    color: #888888;
    font-size: 26rpx;
    text-align: right;
}
.transactions-list .item .item-right {
    flex: 1;
    width: 50%;
    padding-top: 24rpx;
    text-align: right;
}
